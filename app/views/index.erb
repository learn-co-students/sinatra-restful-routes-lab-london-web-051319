<h1>Recipes</h1>
<% @recipes.each do |recipe| %>
  <div>
    <h3><a href="/recipes/<%= recipe.id %>">Recipe: <%= recipe.name %></a></h3>
    <h3>Ingredients:<%= recipe.ingredients %></h3>
    <h3>Cook Time: <%= recipe.cook_time %></h3>
  </div>
<% end %>
<a href="/recipes/new">Create new recipe</a>
